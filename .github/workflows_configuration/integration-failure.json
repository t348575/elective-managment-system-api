[
    {
        "title": "Integration failed",
        "color": 12845619,
        "fields": [
            {
                "name": "Triggered by:",
                "value": "${{ github.actor }}"
            },
            {
                "name": "Event: pull request",
                "value": "[${{ github.sha }}](${{ github.server_url }}/${{ github.repository }}/pull/${{ github.event.number }})"
            },
            {
                "name": "Workflow",
                "value": "[${{ github.workflow }}](${{ github.server_url }}/${{ github.repository }}/actions/runs/${{ github.run_id }})"
            }
        ],
        "timestamp": "${{ steps.current-time.outputs.time }}"
    }
]
